{% if app.request.locale == "ar" %}
    {% set locale = "en" %}
{% else %}
    {% set locale = app.request.locale %}
{% endif %}

<!DOCTYPE html>
<html>
    <head>
        <title>{{ "Order"|trans({}, "messages", locale) }} #{{ order.reference }} - {{ "Subscriptions"|trans({}, "messages", locale) }}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <style>
            *{
                font-family: DejaVu Sans !important;
            }

            table.bordered, table.bordered th, table.bordered td {
                padding: 10px;
                border: 1px solid #eee;
                border-collapse: collapse;
            }
            table tr {
                vertical-align: top;
            }
            table td {
                word-wrap: break-word;
                overflow-wrap: break-word;
            }
            .text-center {
                text-align: center;
            }
            .text-right {
                text-align:right;
            }
            .subscriptions-container .subscription-wrapper {
                page-break-inside: avoid;
            }
            .subscriptions-container .subscription-wrapper:not(:last-child) {
                page-break-after: always;
            }
            .subscriptions-container .subscription-wrapper:not(:first-child) {
                page-break-before: always;
            }
            .text-muted {
                color: #777;
            }
            .footer {
                position:absolute;
                bottom:0;
                width: 100%;
            }
            .scanned-subscription-stamp {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
            }

            .qr-barcode-wrapper > div {
                width: fit-content;
                margin: 0 auto;
            }
        </style>
    </head>
    <body>
        <div class="subscriptions-container">
            {% for orderElement in order.orderelements if (recipeDateSubscriptionReference == "all" or recipeDateSubscriptionReference == orderElement.recipesubscription.reference) and (is_granted("ROLE_ADMIN_APPLICATION") or is_granted("ROLE_CREATOR") or (is_granted("ROLE_RESTAURANT") and orderElement.belongsToRestaurant(app.user.restaurant.slug)) or (is_granted("ROLE_POINTOFSALE") and orderElement.belongsToRestaurant(app.user.pointofsale.restaurant.slug))) %}
                {% for subscription in orderElement.subscriptions %}
                    <div class="subscription-wrapper">
                        <div class="footer text-center text-muted">
                            <div class="qr-barcode-wrapper">
                                {{ barcode({code: order.reference, type: 'c128', format: 'html'}) }}
                            </div>
                            <br>
                            {{ setting.getSettings("website_url") }}
                            <br>
                            {{ setting.getSettings("website_name") }} - {{ setting.getSettings("website_description_" ~ locale) }}
                        </div>
                        <table width="100%" class="bordered">
                            <tbody>
                                <tr>
                                    <td colspan="2" class="text-center">
                                        <img src=".{{ asset(settings.getAppLayoutSettings.getLogoPath) }}" />
                                    </td>
                                </tr>
                                <tr style="background-color: {{ setting.getSettings("primary_color") }}; color: #fff;">
                                    <td width="50%">
                                        <h5>{{ "Order"|trans({}, "messages", locale) }} #{{ order.reference }}</h3>
                                    </td>
                                    <td width="50%">
                                        <h5>{{ "Subscription"|trans({}, "messages", locale) }} #{{ subscription.reference }}</h3>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="50%">
                                        <h3>{{ orderElement.recipesubscription.recipedate.recipe.translate(locale).name }}</h3>
                                        {% if orderElement.recipesubscription.recipedate.startdate %}
                                            <h6><span class="text-muted">{{ "Start date"|trans({}, "messages", locale) }}</span><br>{{ orderElement.recipesubscription.recipedate.startdate|format_datetime('full', 'none', locale=app.request.locale, timezone=date_timezone) }}</h6>
                                            {% endif %}
                                            {% if orderElement.recipesubscription.recipedate.enddate %}
                                            <h6><span class="text-muted">{{ "End date"|trans({}, "messages", locale) }}</span><br>{{ orderElement.recipesubscription.recipedate.enddate|format_datetime('full', 'none', locale=app.request.locale, timezone=date_timezone) }}</h6>
                                            {% endif %}
                                        <h6>
                                            <span class="text-muted">{{ "Venue / Location"|trans({}, "messages", locale) }}</span><br>
                                            {% if orderElement.recipesubscription.recipedate.venue %}
                                                {{ orderElement.recipesubscription.recipedate.venue.translate(locale).name }}
                                            {% else %}
                                                {{ "Online"|trans({}, "messages", locale) }}
                                            {% endif %}
                                        </h6>
                                        {% if orderElement.recipesubscription.recipedate.recipe.restaurant %}
                                            <h6><span class="text-muted">{{ "Restaurant"|trans({}, "messages", locale) }}</span><br>{{ orderElement.recipesubscription.recipedate.recipe.restaurant.name }}</h6>
                                            {% endif %}
                                    </td>
                                    <td width="50%">
                                        <h3>
                                            {{ order.user.getCrossRoleName }}
                                            {% if order.user.hasRole("ROLE_POINTOFSALE") and order.payment.firstname and order.payment.lastname %}
                                                ({{ order.payment.firstname ~ " " ~ order.payment.lastname }})
                                            {% endif %}
                                        </h3>
                                        <h6><span class="text-muted">{{ "Subscription type"|trans({}, "messages", locale) }}</span><br>{{ orderElement.recipesubscription.name }}</h6>
                                        <h6><span class="text-muted">{{ "Subscription price"|trans({}, "messages", locale) }}</span><br>{{ orderElement.recipesubscription.free ? "Free"|trans({}, "messages", locale) : ((settings['currency_position'] == 'left' ? settings['currency_symbol'] : '') ~ orderElement.displayUnitPrice() ~ (settings['currency_position'] == 'right' ? settings['currency_symbol'] : '')) }}</h6>
                                            {% if order.getOrderElementsPriceSum(true) > 0 %}
                                            <h6><span class="text-muted">{{ "Payment method"|trans({}, "messages", locale) }}</span><br>{{ order.paymentgateway.name }}</h6>
                                            {% endif %}
                                        <h6><span class="text-muted">{{ "Order date"|trans({}, "messages", locale) }}</span><br>
                                            {% if order.payment.details["TIMESTAMP"] is defined %}
                                                {{ order.payment.details["TIMESTAMP"]|format_datetime('full', 'none', locale=app.request.locale, timezone=date_timezone) }}
                                            {% elseif order.payment.details["created"] is defined %}
                                                {{ order.payment.details["created"]|format_datetime('full', 'none', locale=app.request.locale, timezone=date_timezone) }}
                                            {% else %}
                                                {{ order.payment.updatedAt|format_datetime('full', 'none', locale=app.request.locale, timezone=date_timezone) }}
                                            {% endif %}
                                        </h6>
                                    </td>
                                </tr>
                                {% if subscription.reservedSeat %}
                                    <tr>
                                        <td class="text-center" colspan="2" style="padding-top: 35px;">
                                            <img style="height: 40px;" src="data:image/png;base64,">
                                            <h4>
                                                {{ services.stringifySeatLabel(subscription.reservedSeat) }}
                                            </h4>
                                        </td>
                                    </tr>
                                {% endif %}
                                <tr>
                                    <td class="text-center" colspan="2">
                                        <div class="qr-barcode-wrapper">
                                            {{ barcode({code: subscription.reference, type: 'qrcode', format: 'html'}) }}
                                        </div>
                                    </td>
                                </tr>
                                {% if subscription.scanned %}
                                    <tr>
                                        <td class="text-center" colspan="2">
                                            <br><br>
                                            <img src=".{{ asset('assets/img/scanned-subscription-stamp.png') }}" height="150" />

                                            <p class="text-center text-muted">{{ "This subscription was scanned at %scanDate%"|trans({"%scanDate%": subscription.updatedAt|format_datetime('full', 'none', locale=app.request.locale, timezone=date_timezone)}, "messages", locale) }}</p>
                                        </td>
                                    </tr>
                                {% endif %}
                            </tbody>
                        </table>
                    </div>
                {% endfor %}
            {% endfor %}
        </div>
    </body>
</html>
