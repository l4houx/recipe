{% if app.request.attributes.get('_route') == 'dashboard_creator_ticket_index' or app.request.attributes.get('_route') == 'dashboard_admin_ticket_index' %}
	{% for ticket in rows %}
		{{ component('ticket', { 'ticket': ticket, 'hidden': true }) }}
	{% endfor %}
{% else %}
	{{ component('ticket', { 'ticket': ticket }) }}
{% endif %}
