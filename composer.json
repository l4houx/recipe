        "presta/sitemap-bundle": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.10",
        "stripe/stripe-php": "^13.14",
        "symfony/asset": "7.0.*",
        "symfony/asset-mapper": "7.0.*",
